  "version": "1.0.0",
  "description": "base api com typescript",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**' --exec \"ts-node src/index.ts\" -e ts",
    "test": "jest --no-cache --detectOpenHandles --config jest.config.js --inspect-brk=42521"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
