{
  "name": "base_api_nodets",
  "version": "1.0.0",
  "description": "base api com typescript",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/' --exec \"ts-node src/index.ts\" -e ts",
    "test": "jest --no-cache"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hc3/base_api_nodets.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hc3/base_api_nodets/issues"
  },
  "homepage": "https://github.com/hc3/base_api_nodets#readme",
  "dependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/supertest": "^2.0.8",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
